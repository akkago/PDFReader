{
  "name": "paddleocr-pdf-reader",
  "version": "1.0.0",
  "description": "PDF text recognition using PaddleOCR",
  "main": "server/index.js",
  "scripts": {
    "start": "node --no-deprecation server/index.js",
    "dev": "nodemon --no-deprecation server/index.js",
    "client": "cd client && npm run serve",
    "client:build": "cd client && npm run build",
    "install:all": "npm install && cd client && npm install"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "fs-extra": "^11.2.0",
    "multer": "^1.4.5-lts.1",
    "path": "^0.12.7",
    "pdf-parse": "^1.1.1",
    "pdf-poppler": "^0.2.1",
    "sharp": "^0.33.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "paddleocr",
    "pdf",
    "text-recognition",
    "vue",
    "nodejs"
  ],
  "author": "",
  "license": "MIT"
}
